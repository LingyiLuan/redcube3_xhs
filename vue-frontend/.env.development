# Development Environment Variables
# Vite loads .env.development when running `npm run dev`

# Backend Service URLs - pointing to local Docker services via API Gateway
VITE_API_GATEWAY_URL=http://localhost:8080
VITE_USER_SERVICE_URL=http://localhost:8080
VITE_CONTENT_SERVICE_URL=http://localhost:8080

# Frontend URL (for OAuth callbacks) - local dev server
VITE_FRONTEND_URL=http://localhost:5173
